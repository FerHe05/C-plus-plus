#include <iostream>
#include <stdio.h>
#include<conio.h>
using namespace std;

//****************Tabela Ascii em C++***************

void ascii() {
	system("color 2");//mudar as letras de cor
	for (int i = 32; i < 255; i++) {
		if (i % 10 == 0) {
			cout << "\n";
		}
	printf("%d = %c\t", i, i);//saida usada em C			
	}
}
int main()
{
	ascii();
	return 0;
}

